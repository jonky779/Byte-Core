
{
  "install": "npm install",
  "start": "npm run start",
  "watch": {
    "install": {
      "include": [
        "^package\\.json$"
      ]
    },
    "restart": {
      "include": [
        "^\\.env$",
        "^server/.*\\.ts$"
      ]
    },
    "throttle": 1000
  },
  "engines": {
    "node": "16.x"
  }
}
